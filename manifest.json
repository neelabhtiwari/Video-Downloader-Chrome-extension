{
  "manifest_version": 2,
  "name": "YouSave",
  "description": "Utility Extension",
  "version": "1.0",
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background":{
      "scripts": ["popup.js"]
    },
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ]
  }
